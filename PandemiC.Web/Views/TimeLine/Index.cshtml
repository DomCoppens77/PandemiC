@model IEnumerable<PandemiC.Web.Models.Forms.TimeLine.TimeLineForm>

@{
    ViewData["Title"] = "Index";
}

@Html.Partial("_DCO_Error")

<p>
    <a asp-action="Create">Create New Time Line</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RestaurantName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DinerDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NbrGuests)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.RestaurantName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DinerDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NbrGuests)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                    @*@Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |*@
                    @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                </td>
            </tr>
        }
    </tbody>
</table>
